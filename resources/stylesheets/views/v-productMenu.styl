						image $base + 'proWhite.png' 12px 12px 
				
					&.against > a
						image $base + 'conWhite.png' 12px 4px

					> a
						image $base + 'proConWhite.png' 12px 17px


				&.attachments
					> a
						image $base + 'paperClipWhite.png' 15px 17px

				&.priceComparison > a
					image $base + 'dollarWhite.png' 12px 16px

				&.coupons > a
