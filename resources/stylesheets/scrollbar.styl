    .lionbars .scrollbar {
        opacity: 0;
        -webkit-transition-property: opacity, width, height;
        -webkit-transition-duration: 200ms, 0ms, 0ms;
        -webkit-transition-delay: 1000ms, 1200ms, 1200ms;
        -moz-transition-property: opacity, width, height;
        -moz-transition-duration: 200ms, 0ms, 0ms;
        -moz-transition-delay: 1000ms, 1200ms, 1200ms;
        -o-transition-property: opacity, width, height;
        -o-transition-duration: 200ms, 0ms, 0ms;
        -o-transition-delay: 1000ms, 1200ms, 1200ms;
        transition-property: opacity, width, height;
        transition-duration: 200ms, 0ms, 0ms;
        transition-delay: 1000ms, 1200ms, 1200ms;
    }
    .lionbars .scrollbar-thumb {
        border-radius: 14px;
    }
    .lionbars.clicked-vertically .scrollbar.vertical .scrollbar-track, .lionbars .scrollbar.vertical:hover .scrollbar-track {
